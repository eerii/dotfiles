(defpoll VOLUME :interval "1s" "~/.cargo/bin/meh --volume loudness")
(defpoll VOLUME_ICON :interval "1s" "~/.cargo/bin/meh --volume icon")

(defpoll BATTERY :interval "1s" "~/.cargo/bin/meh --battery percent")
(defpoll BATTERY_ICON :interval "1s" "~/.cargo/bin/meh --battery icon")

(defpoll WIFI :interval "1s" "echo b")
(defpoll WIFI_ICON :interval "1s" "~/.cargo/bin/meh --network icon")
